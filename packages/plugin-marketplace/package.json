{
  "name": "@bigmind/plugin-marketplace",
  "version": "0.1.0",
  "description": "Plugin marketplace client for BigMind - browse, install and manage plugins",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "bigmind",
    "plugin",
    "marketplace",
    "store"
  ],
  "author": "BigMind Team",
  "license": "MIT",
  "dependencies": {
    "jszip": "^3.10.1",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "@bigmind/plugin-system": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
